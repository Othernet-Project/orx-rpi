config BR2_PACKAGE_PYTHON_LIBRARIAN
	bool "python-librarian"
	depends on BR2_PACKAGE_PYTHON || BR2_PACKAGE_PYTHON3
    select BR2_PACKAGE_PYTHON_LIBRARIAN_CORE
    select BR2_PACKAGE_PYTHON_LIBRARIAN_AUTH
    select BR2_PACKAGE_PYTHON_LIBRARIAN_CONTENT
    select BR2_PACKAGE_PYTHON_LIBRARIAN_DASHBOARD
    select BR2_PACKAGE_PYTHON_LIBRARIAN_MENU
    select BR2_PACKAGE_PYTHON_LIBRARIAN_NOTIFICATIONS
    select BR2_PACKAGE_PYTHON_LIBRARIAN_SETUP
	help
      Content library manager for Outernet Receiver

	  https://github.com/Outernet-Project/librarian/
